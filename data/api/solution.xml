<?xml version="1.0" ?>
<Interface
        xmlns="http://umlfri.org/v2/api.xsd"
        name="org.umlfri.api.model.Solution">
    
    <Documentation>
        Solution - group of all opened projects.
    </Documentation>
    
    <Property name="fileName">
        <Documentation>
            Full path to the project file.
        </Documentation>
        
        <Value type="string" readable="true" nullable="true"/>
    </Property>
    
    <Property name="projects">
        <Documentation>
            Child projects.
        </Documentation>
        
        <Value type="org.umlfri.api.model.Project" iterable="true"/>
    </Property>
</Interface>
