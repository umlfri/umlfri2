<?xml version="1.0" ?>
<Exception
        xmlns="http://umlfri.org/v2/api.xsd"
        name="org.umlfri.api.metamodel.PartNotFound">
    
    <Documentation>
        The part of the metamodel with a given identification was not found.
    </Documentation>
    
    <Property name="name">
        <Documentation>
            The part identificator
        </Documentation>
        
        <Value type="string" readable="true"/>
    </Property>
    
    <Stringify>
        <Text text="The metamodel part with name " />
        <Property name="name" />
        <Text text=" was not found" />
    </Stringify>
</Exception>
