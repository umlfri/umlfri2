<?xml version="1.0" encoding="utf-8"?>
<DiagramType id="use_case_diagram" xmlns="http://umlfri.org/v2/addon.xsd">
    <Icon path="icons/use_case_diagram.png"/>

    <Structure>
        <Attribute id="name" type="str">
            <Template>
                <Text text="#parent"/>
            </Template>
        </Attribute>
        <Attribute id="note" type="text"/>
    </Structure>

    <DisplayName>
        <Text text="#self.name"/>
    </DisplayName>

    <Elements>
        <Element id="actor"/>
    </Elements>

    <Connections/>

    <Appearance>
        <Background color="#cfg.background"/>
    </Appearance>
</DiagramType>
